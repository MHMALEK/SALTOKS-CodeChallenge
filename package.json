{
   "name": "CodeChallenge",
   "version": "1.0.0",
   "keywords": [
      "webpack",
      "react"
   ],
   "description": "a code challenge for Salto KS company",
   "main": "index.js",
   "scripts": {
      "start": "NODE_ENV=development webpack-dev-server --config setup/webpack/webpack.dev.js",
      "test": "jest --config ./jest.config.json",
      "end2end": "cypress open",
      "build:test": "webpack --config setup/webpack/webpack.prod.js",
      "build:production": "NODE_ENV=production webpack --config setup/webpack/webpack.prod.js --progress --hide-modules",
      "analyze": "source-map-explorer dist/'*.js'",
      "lint": "eslint .",
      "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
   },
   "repository": "https://gitlab.com/mhos.malek/salto-ks-test.git",
